alt - h : yabai -m window --focus west
alt - j : yabai -m window --focus south
alt - k : yabai -m window --focus north
alt - l : yabai -m window --focus east
ctrl + alt - h : yabai -m window --swap west
ctrl + alt - j : yabai -m window --swap south
ctrl + alt - k : yabai -m window --swap north
ctrl + alt - l : yabai -m window --swap east
alt - r : yabai -m space --rotate 90
alt - f : yabai -m window --toggle zoom-fullscreen
alt - t : yabai -m window --toggle float
alt - e : yabai -m window --toggle split
shift + alt - 0 : yabai -m space --balance
ctrl + alt - e : yabai -m window --grid 1:3:0:1:2:1
ctrl + alt - t : yabai -m window --grid 1:3:1:1:2:1
ctrl + alt - d : yabai -m window --grid 1:3:0:1:1:1
ctrl + alt - f : yabai -m window --grid 1:3:1:1:1:1
ctrl + alt - g : yabai -m window --grid 1:3:2:1:1:1
ctrl + alt - return : yabai -m window --grid 1:1:0:0:1:1
alt + shift - w : yabai -m window --close
alt + shift - n : yabai -m space --focus next
alt + shift - p : yabai -m space --focus prev
ctrl + alt - n : yabai -m window --space next
ctrl + alt - p : yabai -m window --space prev
ctrl + alt + shift - n : yabai -m space --create
